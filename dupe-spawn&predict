local TeleportService = game:GetService("TeleportService")
local placeId = YOUR_ACTUAL_PLACE_ID_HERE  -- Replace with correct place ID
local accessCode = "ece247e419febc4c8b29d8311b93cc59"  -- Your server's access code
local player = game.Players.LocalPlayer

local function joinPrivateServer(player)
    TeleportService:TeleportToPrivateServer(placeId, accessCode, {player})
end

script.Parent.Touched:Connect(function(hit)
    local player = game.Players:GetPlayerFromCharacter(hit.Parent)
    if player then
        joinPrivateServer(player)
    end
end)
